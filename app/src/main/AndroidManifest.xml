<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.depaul.csc472.m.mcclellan.mcclellanmracketbuilder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_mylauncher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ListActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BuildActivity"
            android:screenOrientation="portrait"
            android:label="Build Your Racket">
        </activity>
        <activity
            android:name=".AppraiseActivity"
            android:screenOrientation="portrait"
            android:label="Racket Appraisal">
        </activity>
        <activity
            android:name=".InfoActivity"
            android:screenOrientation="portrait"
            android:label="Info">
        </activity>
    </application>

</manifest>